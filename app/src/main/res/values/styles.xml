<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="APTheme" parent="AppTheme">
        <item name="android:statusBarColor">#1A629B</item>
    </style>


    <!--动画方式-->
    <style name="xxxx" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/mima_in</item>
        <item name="android:windowExitAnimation">@anim/dialog_payview_out</item>
    </style>
    <declare-styleable name="MdStyleProgress">
        <attr name="progress_color" format="color"/>
        <attr name="progress_width" format="dimension"/>
        <attr name="radius" format="dimension"/>
    </declare-styleable>




</resources>
